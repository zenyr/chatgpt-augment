{
  "manifest_version": 3,
  "name": "ChatGPT DOM Mutation Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["chatgpt-augment.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon128.png",
    "64": "icon64.png"
  }
}
